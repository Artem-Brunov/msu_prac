
var text;
var synth;
var ttsObject;


function add()
{	
	var documentsDir;
	function onResolveSuccess(dir) {
	documentsDir = dir;
	}
	 
	function onResolveError(e) {
	console.log('message: ' + e.message);
	}
	    

}

function talk() 
{

	synth = window.speechSynthesis;
